{
  "name": "nafo",
  "version": "1.0.0",
  "description": "Theme NAFO",
  "author": "Roman Kulish",
  "license": "MIT",
  "homepage": "https://github.com/roman-kulish/nafo-au-landing.git",
  "keywords": [
    "hugo",
    "gohugo",
    "nafo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roman-kulish/nafo-au-landing.git"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "test": "yarn run test:eslint && yarn run test:prettier && yarn run test:html",
    "format": "yarn run format:eslint && yarn run format:prettier && yarn run format:html",
    "test:eslint": "eslint --ext .ts --ignore-path .gitignore .",
    "format:eslint": "eslint --fix --ext .ts --ignore-path .gitignore .",
    "test:prettier": "prettier --ignore-path .gitignore --check '**/*.{scss,css,ts,yml}'",
    "format:prettier": "prettier --ignore-path .gitignore --write '**/*.{scss,css,ts,yml}'",
    "test:html": "prettier --check 'layouts/**/*.html'",
    "format:html": "prettier --write 'layouts/**/*.html'"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": ">=2.25.4",
    "eslint-plugin-node": ">=7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": ">=6.0.0",
    "eslint-plugin-standard": ">=5.0.0",
    "plyr": "^3.7.8",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-go-template": "^0.0.13",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "flowbite": "^1.6.4"
  }
}
